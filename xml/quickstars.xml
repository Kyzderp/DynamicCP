<GuiXml>
    <Controls>
        <TopLevelControl name="DynamicCPQuickstarsContainer" mouseEnabled="false" movable="false" hidden="false">
            <Controls>
<!-- ======================================================================================================================= -->
                <Control name="DynamicCPQuickstars" mouseEnabled="true" movable="true" hidden="false">
                    <Dimensions x="300" y="300"/>
                    <OnMoveStop>DynamicCP.SaveQuickstarsPosition()</OnMoveStop>
                    <Controls>
                        <Control name="$(parent)GreenButton" mouseEnabled="true">
                            <Dimensions x="32" y="32"/>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT"/>
                            <OnMouseUp>DynamicCP.SelectTab("Green")</OnMouseUp>
                            <Controls>
                                <Backdrop name="$(parent)Backdrop" alpha="0.5" centerColor="FF000000" edgeColor="00000000"><AnchorFill/></Backdrop>
                                <Texture name="$(parent)Icon" textureFile="esoui/art/champion/champion_points_stamina_icon-hud-32.dds" tier="HIGH">
                                    <Dimensions x="24" y="24"/>
                                    <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
                                </Texture>
                            </Controls>
                        </Control>
                        <Control name="$(parent)BlueButton" mouseEnabled="true">
                            <Dimensions x="32" y="32"/>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)GreenButton" relativePoint="BOTTOMLEFT"/>
                            <OnMouseUp>DynamicCP.SelectTab("Blue")</OnMouseUp>
                            <Controls>
                                <Backdrop name="$(parent)Backdrop" alpha="0.5" centerColor="FF000000" edgeColor="00000000"><AnchorFill/></Backdrop>
                                <Texture name="$(parent)Icon" textureFile="esoui/art/champion/champion_points_magicka_icon-hud-32.dds" tier="HIGH">
                                    <Dimensions x="24" y="24"/>
                                    <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
                                </Texture>
                            </Controls>
                        </Control>
                        <Control name="$(parent)RedButton" mouseEnabled="true">
                            <Dimensions x="32" y="32"/>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)BlueButton" relativePoint="BOTTOMLEFT"/>
                            <OnMouseUp>DynamicCP.SelectTab("Red")</OnMouseUp>
                            <Controls>
                                <Backdrop name="$(parent)Backdrop" alpha="0.5" centerColor="FF000000" edgeColor="00000000"><AnchorFill/></Backdrop>
                                <Texture name="$(parent)Icon" textureFile="esoui/art/champion/champion_points_health_icon-hud-32.dds" tier="HIGH">
                                    <Dimensions x="24" y="24"/>
                                    <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
                                </Texture>
                            </Controls>
                        </Control>
<!-- ================================================================================================== -->
                        <Control name="$(parent)List">
                            <Dimensions x="208" y="144"/>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)GreenButton" relativePoint="TOPRIGHT"/>
                            <Controls>
                                <Backdrop name="$(parent)Backdrop" alpha="0.5" centerColor="FF000000" edgeColor="00000000"><AnchorFill/></Backdrop>
                                <Control name="$(parent)Star1" inherits="ZO_ComboBox" tier="HIGH">
                                    <Dimensions x="200"/>
                                    <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="4" offsetY="4"/>
                                </Control>
                                <Control name="$(parent)Star2" inherits="ZO_ComboBox" tier="HIGH">
                                    <Dimensions x="200"/>
                                    <Anchor point="TOPLEFT" relativeTo="$(parent)Star1" relativePoint="BOTTOMLEFT" offsetY="4"/>
                                </Control>
                                <Control name="$(parent)Star3" inherits="ZO_ComboBox" tier="HIGH">
                                    <Dimensions x="200"/>
                                    <Anchor point="TOPLEFT" relativeTo="$(parent)Star2" relativePoint="BOTTOMLEFT" offsetY="4"/>
                                </Control>
                                <Control name="$(parent)Star4" inherits="ZO_ComboBox" tier="HIGH">
                                    <Dimensions x="200"/>
                                    <Anchor point="TOPLEFT" relativeTo="$(parent)Star3" relativePoint="BOTTOMLEFT" offsetY="4"/>
                                </Control>

                                <Control name="$(parent)Confirm">
                                    <Dimensions x="28" y="28"/>
                                    <Anchor point="BOTTOMLEFT" relativeTo="$(parent)" relativePoint="BOTTOMRIGHT"/>
                                    <Controls>
                                        <Backdrop name="$(parent)Backdrop" alpha="0.5" centerColor="FF000000" edgeColor="00000000"><AnchorFill/></Backdrop>
                                        <Button name="$(parent)Button" topmost="true">
                                            <Textures normal="/esoui/art/buttons/accept_up.dds" mouseOver="/esoui/art/buttons/accept_over.dds" />
                                            <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
                                            <Dimensions x="22" y="22" />
                                        </Button>
                                    </Controls>
                                </Control>
                                <Control name="$(parent)Cancel">
                                    <Dimensions x="28" y="28"/>
                                    <Anchor point="BOTTOMLEFT" relativeTo="$(parent)Confirm" relativePoint="TOPLEFT"/>
                                    <Controls>
                                        <Backdrop name="$(parent)Backdrop" alpha="0.5" centerColor="FF000000" edgeColor="00000000"><AnchorFill/></Backdrop>
                                        <Button name="$(parent)Button" topmost="true">
                                            <Textures normal="/esoui/art/buttons/decline_up.dds" mouseOver="/esoui/art/buttons/decline_over.dds" />
                                            <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
                                            <Dimensions x="22" y="22" />
                                        </Button>
                                    </Controls>
                                </Control>
                            </Controls>
                        </Control>
                    </Controls>
                </Control>
<!-- ======================================================================================================================= -->
            </Controls>
        </TopLevelControl>
    </Controls>
</GuiXml>